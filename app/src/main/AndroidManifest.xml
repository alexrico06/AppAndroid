<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madridizate2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Madridizate2">
        <activity android:name=".ReservaParking"></activity>
        <activity android:name=".RegistrarVehiculoActivity" />
        <activity android:name=".RegistrarUserActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_main3"
            android:theme="@style/Theme.Madridizate2.NoActionBar" />
    </application>

</manifest>